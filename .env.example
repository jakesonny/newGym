# 서버 설정
PORT=3001
NODE_ENV=development
REQUEST_TIMEOUT=30000

# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=postgres
DB_NAME=gym_membership_db
DB_SYNCHRONIZE=false
DB_LOGGING=true

# Render 등 외부 DB 연결 (DATABASE_URL이 있으면 우선 사용)
# DATABASE_URL=postgresql://user:password@host:port/database

# 보안 설정 (JWT)
JWT_SECRET=your-very-secret-key-change-me
JWT_EXPIRES_IN=7d

# CORS 설정 (쉼표로 구분하여 여러 URL 허용 가능)
FRONTEND_URL=http://localhost:3000,https://gym-kappa-nine.vercel.app

# 외부 서비스 연동 (카카오 로그인 등)
# KAKAO_CLIENT_ID=
# KAKAO_CALLBACK_URL=
